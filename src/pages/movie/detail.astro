---
import Layout from '@/layouts/Layout.astro';
import { baseApiUrl } from '@/api/index.ts';
interface Props {
	title: string
}
interface Params {
	[key:string]: string | number | undefined
}
const { title } = Astro.props;
const { id } = Astro.params as Params;
let indexData:any = {};
const getData = async () => {
    let res = await fetch(baseApiUrl + '/movie/detail?id=1');
    let data = await res.json();
    indexData = data.data;
    console.log(indexData, 'res');
};
await getData();
---
<Layout title={`电影详情-${title?title+'-':''}`}>
	<details>{indexData}</details>
</Layout>
