---
import Tabs from '@/components/Movie/Tabs.vue';
import List from '@/components/Movie/List.tsx';
import Layout from '@/layouts/Layout.astro';
import { baseApiUrl } from '@/api/index.ts';
import { Card, Row, Col } from 'antd';
let indexData:any = {};
const getData = async () => {
    let res = await fetch(baseApiUrl + '/movie');
    let data = await res.json();
    indexData = data.data;
    console.log(indexData, 'res');
};
await getData();
---
<Layout title='猫眼电影'>
    <Tabs client:load></Tabs>
    <List client:load data={indexData}></List>
</Layout>