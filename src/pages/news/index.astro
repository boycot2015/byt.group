---
import CateList from '@/components/music/playlist/cate.svelte';
import PlayList from '@/components/news/index.vue';
import Layout from '@/layouts/Layout.astro';
import { KContainer } from '@ikun-ui/core';
interface Props {
	title?: string
}
interface Params {
	[key:string]: string
}
const typeName: Params = {
	'qq': 'QQ音乐',
	'netease': '网易云音乐',
	'kugou': '酷狗音乐',
	'kuwo': '酷我音乐',
	'qianqian': '千千音乐',
	'migu': '咪咕音乐',
}
const { title } = Astro.props;
const { type = 'netease' } = Astro.params;

---
<Layout title={`新闻-首页`}>
  <KContainer cls='flex-col overflow-hidden h-[100%]'>
    <PlayList client:only="vue" {type}></PlayList>
  </KContainer>
</Layout>
