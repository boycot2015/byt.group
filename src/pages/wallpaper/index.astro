---
import List from '@/components/Solid/wallpaper.jsx';
import Layout from '@/layouts/Layout.astro';
import { KContainer } from '@ikun-ui/core';
import ImageViewer from '@/components/ImageViewer/index.vue';
interface Props {
	title?: string
}
interface Params {
	[key:string]: string
}
const { type = 'birdpaper' } = Astro.params;

---
<Layout title={`壁纸`}>
  <KContainer cls='flex-col h-[100%] mx-auto max-w-[1920px]'>
    <List client:load {type}></List>
    <ImageViewer client:load></ImageViewer>
  </KContainer>
</Layout>
<style>
</style>
